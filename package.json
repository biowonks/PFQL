{
  "name": "fql",
  "version": "1.0.0",
  "description": "Feature query language",
  "main": ".eslintrc.js",
  "scripts": {
    "test": "NODE_PATH=node_modules node_modules/.bin/nyc node_modules/.bin/mocha --reporter ${MOCHA_REPORTER-spec} $(find src -name '*tests.js') -r ../_common/test.globals.js",
    "coverage": "NODE_PATH=node_modules node_modules/.bin/istanbul cover --config ../_common/.istanbul.js node_modules/mocha/bin/_mocha -- --reporter ${MOCHA_REPORTER-spec} -r ../_common/test.globals.js '*.tests.js'",
    "cover": "NODE_PATH=node_modules istanbul cover ./node_modules/mocha/bin/_mocha -r ../_common/test.globals.js --report lcovonly -- -R spec *tests.js && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "eslint": "node_modules/.bin/eslint '*.js'",
    "performance": "node --allow-natives-syntax ./node_modules/mocha/bin/_mocha --reporter mocha-performance *tests.js"
  },
  "nyc": {
    "exclude": [
      "src/**tests.js"
    ]
  },
  "author": "BioWonks",
  "license": "",
  "contributors": [
    {
      "name": "Luke Ulrich"
    },
    {
      "name": "Ogun Adebali"
    },
    {
      "name": "Davi Ortega"
    },
    {
      "name": "Amit Upadhyay"
    }
  ],
  "dependencies": {
    "JSONStream": "^1.3.1",
    "async": "^2.1.5",
    "bunyan": "^1.8.8",
    "pumpify": "^1.3.5",
    "request": "^2.75.0",
    "through2": "^2.0.3"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^2.11.15",
    "eslint": "^3.14.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "mocha-performance": "^0.1.1",
    "nyc": "^11.2.1",
    "sinon": "^1.17.7"
  }
}
